<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:XamarinFirstProject"
             x:Class="XamarinFirstProject.Views.UsingImages">
    <ContentPage.Content>
        <StackLayout>
            
            <Image Source="JabaFat.png" HeightRequest="150" WidthRequest="150"></Image>
            
            <Image x:Name="imageLogo" HeightRequest="150" WidthRequest="150"></Image>

            <Image Source="https://upload.wikimedia.org/wikipedia/en/thumb/1/12/Chewbaca_%28Peter_Mayhew%29.png/220px-Chewbaca_%28Peter_Mayhew%29.png"></Image>
            
            <Image Source="{local:ImageResourceExtention Source=XamarinFirstProject.Images.bobaFett.jpg}"
                   HeightRequest="150" WidthRequest="150"></Image>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>